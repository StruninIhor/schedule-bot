{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Nginx": {
    "PathBase": "/somepath"
  },
  "ConnectionStrings": {
    "BotDb": "Host=<HOST>; Port=<PORT>; Database=<DATABASE_NAME>; Username=<USERNAME>; Password=<PASSWORD>"
  },
  "BotConfiguration": {
    "BotToken": "<YOUR_BOT_TOKEN>"
  },
  "BotMessageConfiguration": {
    "AdminId": "<ADMIN_CHAT_ID>",
    "LessonsCommand": "What is lesson(s) now?"
  }
}
